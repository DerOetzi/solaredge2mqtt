# SolarEdge2MQTT Secrets File
# This file contains sensitive configuration values.
# IMPORTANT: Keep this file secure and DO NOT commit it to version control!

# Secrets can be referenced in configuration.yml using !secret tag
# Example: password: !secret mqtt_password

# MQTT password
mqtt_password: "your_mqtt_password_here"

# SolarEdge Monitoring Platform site ID and password (optional)
# monitoring_site_id: "12345678"
# monitoring_password: "your_monitoring_password_here"

# SolarEdge Wallbox password and serial (optional)
# wallbox_password: "your_wallbox_admin_password_here"
# wallbox_serial: "ABC123456"

# InfluxDB token (optional)
# influxdb_token: "your_influxdb_token_here"

# OpenWeatherMap API key (optional)
# weather_api_key: "your_openweathermap_api_key_here"
